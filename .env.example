# Environment Configuration
# Copy this file to .env.local and update with real values

# Gate System API (untuk data kunjungan)
REACT_APP_GATE_API_URL=http://localhost:8080/api/gate
REACT_APP_GATE_API_KEY=your-gate-api-key

# SLiMS API (untuk data peminjaman & buku)
REACT_APP_SLIMS_API_URL=http://localhost/slims/api
REACT_APP_SLIMS_API_KEY=your-slims-api-key

# Mode: 'dummy' untuk data dummy, 'production' untuk API real
REACT_APP_DATA_MODE=dummy

# Refresh interval (milliseconds)
REACT_APP_REFRESH_INTERVAL=300000
